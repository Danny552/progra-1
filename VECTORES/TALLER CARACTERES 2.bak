#lang racket
(define (nota)
  (define n (read-char))
  (if (char=? n #\E)
      (display "Excelente")
      (if(char=? n #\B)
         (display "Bueno")
         (if(char=? n #\A)
            (display "Aceptable")
            (if(char=? n #\D)
               (display "Deficiente")
               (if(char=? n #\I)
                  (display "Insuficiente")
                  (display "ERROR")
                  )
               )
            )
         )
      )
  )
